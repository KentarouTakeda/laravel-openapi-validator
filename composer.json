{
    "name": "kentaroutakeda/laravel-openapi-validator",
    "description": "Laravel OpenAPI Validator",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "KentarouTakeda\\Laravel\\OpenApiValidator\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.17",
        "friendsofphp/php-cs-fixer": "^3.41",
        "phpunit/php-code-coverage": "^10.1",
        "phpstan/phpstan": "^1.10"
    },
    "autoload-dev": {
        "psr-4": {
            "KentarouTakeda\\Laravel\\OpenApiValidator\\Tests\\": "tests"
        }
    },
    "config": {
        "process-timeout": 0,
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "captainhook/captainhook-phar": false
        }
    },
    "require": {
        "laravel/framework": "^10.38",
        "league/openapi-psr7-validator": "^0.21.0",
        "nyholm/psr7": "^1.8",
        "symfony/psr-http-message-bridge": "^7.0",
        "vyuldashev/laravel-openapi": "^1.11"
    }
}
